  "scripts": {
    "apolloserv": "node ./src/server/apollo/server.js",
    "start": "NODE_ENV=production node ./dist/server/server.js",
    "build": "npm run buildServJS && npm run convert_conf && webpack --mode production --env.NODE_ENV=production",
    "buildServJS": "flow-remove-types ./src/server/ -d ./dist/server/ && cp -rf ./src/server/libs/views/ ./dist/server/libs/views",
    "build_dev_db_container": "docker build ./init/. -t pg-icoripper-dev",
    "convert_conf": "node ./dist/server/libs/routes/utils/runConvert.js",
    "createConfigForFront": "node ./dist/server/libs/routes/utils/createConfigForFront.js",
    "dev": "npm run buildServJS && npm run convert_conf && NODE_ENV=development babel-node ./dist/server/server.js && webpack --mode development --env.NODE_ENV=development",
    "dev_clear_uploads": "./config/clear-uploads-folder.sh",
    "dev_restart_db": "npm run build_dev_db_container && ./config/docker-development-stop.sh && ./config/docker-development-start.sh",
    "flow:check": "flow check ./src/ --all --pretty",
    "flow:deps": "flow-typed install",
    "flow": "flow",
    "jest_cov": "jest",
    "jest": "jest --watch",
    "puppeteer": "ava ./test/puppeteer/statistics.js -v",
    "serverdev": "NODE_ENV=development WILDCARD_HOSTNAME=localhost node ./dist/server/server.js",
    "webpack": "npm run buildServJS && NODE_ENV=development npm run createConfigForFront  &&  webpack --mode development --env.NODE_ENV=development",
    "test": "npm run build && ./test/puppeteer/test.sh"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/jest-css-modules"
