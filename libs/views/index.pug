- var clearCacheQuery = environment !== 'production' ? `?t=${+ new Date()}` : '';
doctype html
html
    head
        title!= `Partner cabinet`
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        meta(http-equiv='Cache-Control' content='no-cache')
        link(rel='stylesheet', href=`/css/font-awesome/css/font-awesome.min.css${clearCacheQuery}`)
        link(rel='stylesheet', href=`/css/bootstrap.css${clearCacheQuery}`)
    body
        div(id='root')
        script(src=`/js/bundle.js${clearCacheQuery}`)
        